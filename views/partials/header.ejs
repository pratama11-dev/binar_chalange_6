<header class="border-bottom py-2 mb-4">
  <nav class="navbar navbar-expand-sm">
    <div class="container">
      <a class="navbar-brand" href="/">Panel Admin</a>

      <button
        class="navbar-toggler"
        type="button"
        data-bs-toggle="collapse"
        data-bs-target="#navbarSupportedContent"
        aria-controls="navbarSupportedContent"
        aria-expanded="false"
        aria-label="Toggle navigation"
      >
        <span class="navbar-toggler-icon"></span>
      </button>

      <div class="collapse navbar-collapse" id="navbarSupportedContent">
        <ul class="navbar-nav ms-auto">
          <li class="nav-item">
            <% if (url.includes('/home')) { %>
            <a href="/login" class="nav-link" aria-current="page">Login</a>
          </li>
          <% } else { %>
          <li class="nav-item">
            <% if (url.includes('/gameusers')) { %>
            <a href="/gameusers" class="nav-link active" aria-current="page"
              >Users Game</a
            >
            <% } else { %>
            <a href="/gameusers" class="nav-link">Users Game</a>

            <% } %>
          </li>

          <li class="nav-item">
            <% if (url.includes('/histories')) { %>
            <a href="/histories" class="nav-link active" aria-current="page"
              >Histori Game</a
            >
            <% } else { %>
            <a href="/histories" class="nav-link">Histori Game</a>

            <% } %>
          </li>
          <li class="nav-item">
            <a href="/home" class="nav-link" aria-current="page">Logout</a>
          </li>
          <% } %>
        </ul>
      </div>
    </div>
  </nav>
</header>
